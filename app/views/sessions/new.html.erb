<h1>Log In</h1>

<%= render 'partials/errors' %>

<%= form_tag sessions_path do %>
  <%= label_tag :username %>
  <%= text_field_tag :username %>

  <%= label_tag :password %>
  <%= password_field_tag :password %>

  <%= submit_tag "Log in"%>
<% end %>

<br>
<%= button_to "Register", login_path, method: :get %>
